qual:
  rdf:type owl:Ontology ;
  dc:source "natsoil CSIRO database - texture qualification table"^^xsd:string ;
  dct:created "2015-11-30"^^xsd:date ;
  dct:creator <mailto:xavier.butcher@csiro.au> ;
  rdfs:comment "The non-clay field texture grades (clay loams and coarser) may be qualified according to whether they are at, or near, the light (lower clay content) or heavy (higher clay content) end of the range for that particular field texture grade. They can also be classified as Fibric or Sapric."^^xsd:string ;
  rdfs:label "Field Texture Qualification"^^xsd:string ;
  rdfs:seeAlso "National Committee on Soil and Terrain (2009), ‘Australian soil and land survey field handbook (3rd edn).’ (CSIRO Publishing: Melbourne)"^^xsd:string ;
  owl:imports dc: ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.
