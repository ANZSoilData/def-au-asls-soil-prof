# baseURI: http://registry.it.csiro.au/def/soils/root-size/

@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdf123: <http://rdf123.umbc.edu/ns/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix root_size: <http://registry.it.csiro.au/def/soils/root-size/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

root_size:
  rdf:type owl:ontology ;
  dc:source "National Committee on Soil and Terrain (2009), 'Australian soil and land survey field handbook (3rd edn).' (CSIRO Publishing: Melbourne)"^^xsd:string ;
  dct:created "2015-12-18"^^xsd:date ;
  dct:creator <mailto:xavier.butcher@csiro.au> ;
  dct:description "Record the presence of roots observed in each horizon in areas 100 square millimetres on a cleaned exposure face."^^xsd:string ;
  rdfs:comment "loaded from natsoil CSIRO database - Root Size table"^^xsd:string ;
  rdfs:label "Root Size"^^xsd:string ;
.
root_size:coarse
  rdf:type skos:Concept ;
  dct:description "Coarse, >5mm" ;
  rdfs:label "Coarse" ;
  xsd:minInclusive "5" ;
  skos:altLabel "Coarse" ;
  skos:definition "Coarse, >5mm" ;
  skos:notation "4" ;
  skos:prefLabel "Coarse"@en ;
.
root_size:fine
  rdf:type skos:Concept ;
  dct:description "Fine, 1-2mm" ;
  rdfs:label "Fine" ;
  xsd:maxExclusive "2" ;
  xsd:minInclusive "1" ;
  skos:altLabel "Fine" ;
  skos:definition "Fine, 1-2mm" ;
  skos:notation "2" ;
  skos:prefLabel "Fine"@en ;
.
root_size:medium
  rdf:type skos:Concept ;
  dct:description "Medium, 2-5mm" ;
  rdfs:label "Medium" ;
  xsd:maxExclusive "5" ;
  xsd:minInclusive "2" ;
  skos:altLabel "Medium" ;
  skos:definition "Medium, 2-5mm" ;
  skos:notation "3" ;
  skos:prefLabel "Medium"@en ;
.
root_size:very-fine
  rdf:type skos:Concept ;
  dct:description "Very Fine, <1mm" ;
  rdfs:label "Very Fine" ;
  xsd:maxExclusive "1" ;
  xsd:minInclusive "0" ;
  skos:altLabel "Very Fine" ;
  skos:definition "Very Fine, <1mm" ;
  skos:notation "1" ;
  skos:prefLabel "Very Fine"@en ;
.
