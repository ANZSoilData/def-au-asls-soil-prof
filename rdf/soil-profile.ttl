  skos:prefLabel "Self-mulching"@en ;
.
sp:surfacecondition-O
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/soil-profile/surfacecondition-O"^^xsd:anyURI ;
  skos:definition "Other"@en ;
  skos:notation "O" ;
  skos:prefLabel "Other (surface condition)"@en ;
.
sp:surfacecondition-P
