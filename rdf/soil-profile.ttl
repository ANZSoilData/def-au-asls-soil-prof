sp:pans-cementation-1
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/soil-profile/pans-cementation-1"^^xsd:anyURI ;
  rdfs:comment "C_PAN_CEMENTATION" ;
