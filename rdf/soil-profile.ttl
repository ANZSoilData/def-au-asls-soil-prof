  skos:prefLabel "Weakly cemented"@en ;
.
sp:pans-cementation-2
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/soil-profile/pans-cementation-2"^^xsd:anyURI ;
  rdfs:comment "C_PAN_CEMENTATION" ;
  skos:definition "beyond power of thumb and forefinger. Crushes underfoot on hard, flat surface with weight of average person (80 kg) applied slowly."@en ;
  skos:notation "2" ;
  skos:prefLabel "Moderately cemented"@en ;
.
sp:pans-cementation-3
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/soil-profile/pans-cementation-3"^^xsd:anyURI ;
  rdfs:comment "C_PAN_CEMENTATION" ;
  skos:definition "cannot be crushed underfoot by weight of average man applied slowly. Can be broken by hammer."@en ;
  skos:notation "3" ;
  skos:prefLabel "Strongly cemented"@en ;
.
sp:pans-cementation-4
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/soil-profile/pans-cementation-4"^^xsd:anyURI ;
  rdfs:comment "C_PAN_CEMENTATION" ;
  skos:definition "cannot be broken by hammer, or only with extreme difficulty."@en ;
  skos:notation "4" ;
  skos:prefLabel "Very strongly cemented"@en ;
.
