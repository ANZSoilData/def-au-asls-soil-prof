# imports: http://www.w3.org/2004/02/skos/core
# prefix: consistence

@prefix consistence: <http://registry.it.csiro.au/sandbox/soil/consistence/strength/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdf123: <http://rdf123.umbc.edu/ns/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dc:
  dct:description "This version has been modified for use in TopBraid: it defines the dc namespace prefix and imports SKOS and DCTerms."^^xsd:string ;
  rdfs:label "Dublin Core Metadata Element Set, Version 1.1" ;
.
consistence:
  rdf:type owl:Ontology ;
  dc:source "National Committee on Soil and Terrain (2009), 'Australian soil and land survey field handbook (3rd edn).' (CSIRO Publishing: Melbourne)"^^xsd:string ;
  dct:created "2017-08-18"^^xsd:date ;
  dct:creator <mailto:asris@csiro.au> ;
  dct:description "Strength of soil is the resistance to breaking or deformation. Stength is determined by the force just sufficient to break or deform a 20 mm diameter piece of soil when a compressive shearing force is applied between thumb and forefinger. The 20 mm piece of soil may be a ped, part of a ped, a compound ped or a fragment."^^xsd:string ;
  dct:license <http://creativecommons.org/licences/by/4.0> ;
  dct:publisher <http://www.publish.csiro.au/> ;
  dct:rights "copyright CSIRO 2009, 2016. All rights reserved." ;
  rdfs:label "Strength"^^xsd:string ;
  rdfs:seeAlso <http://www.publish.csiro.au/nid/22/pid/5230.htm> ;
  owl:imports dc: ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.
consistence:firm
  rdf:type skos:Concept ;
  dct:description "Moderate or firm force."^^xsd:string ;
  rdfs:label "Firm"^^xsd:string ;
  skos:definition "Moderate or firm force."^^xsd:string ;
  skos:notation "3"^^xsd:string ;
  skos:prefLabel "Firm"@en ;
.
consistence:loose
  rdf:type skos:Concept ;
  dct:description "No force required. Separate particles such as loose sands."^^xsd:string ;
  rdfs:label "Loose" ;
  skos:definition "No force required. Separate particles such as loose sands."^^xsd:string ;
  skos:notation "0"^^xsd:string ;
  skos:prefLabel "Loose"@en ;
.
consistence:rigid
  rdf:type skos:Concept ;
  dct:description "Cannot be crushed underfoot by full body weight applied slowly."^^xsd:string ;
  rdfs:label "Rigid" ;
  skos:definition "Cannot be crushed underfoot by full body weight applied slowly."^^xsd:string ;
  skos:notation "7" ;
  skos:prefLabel "Rigid"@en ;
.
consistence:strong
  rdf:type skos:Concept ;
  dct:description "Beyond power of thumb and forefinger. Crushes underfoot on hard, flat surface with small force."^^xsd:string ;
  rdfs:label "Strong" ;
  skos:definition "Beyond power of thumb and forefinger. Crushes underfoot on hard, flat surface with small force."^^xsd:string ;
  skos:notation "5" ;
  skos:prefLabel "Strong"@en ;
.
consistence:very-firm
  rdf:type skos:Concept ;
  dct:description "Strong force but within power of thumb and forefinger."^^xsd:string ;
  rdfs:label "Very Firm" ;
  skos:definition "Strong force but within power of thumb and forefinger."^^xsd:string ;
  skos:notation "4" ;
  skos:prefLabel "Very Firm"@en ;
.
consistence:very-strong
  rdf:type skos:Concept ;
  dct:description "Crushes underfoot on hard, flat surface with full body weight applied slowly."^^xsd:string ;
  rdfs:label "Very Strong" ;
  skos:definition "Crushes underfoot on hard, flat surface with full body weight applied slowly."^^xsd:string ;
  skos:notation "6" ;
  skos:prefLabel "Very Strong"@en ;
.
consistence:very-weak
  rdf:type skos:Concept ;
  dct:description "Very small force, almost nil."^^xsd:string ;
  rdfs:label "Very weak" ;
  skos:definition "Very small force, almost nil."^^xsd:string ;
  skos:notation "1" ;
  skos:prefLabel "Very weak"@en ;
.
consistence:weak
  rdf:type skos:Concept ;
  dct:description "Small but significant force."^^xsd:string ;
  rdfs:label "Weak" ;
  skos:definition "Small but significant force."^^xsd:string ;
  skos:notation "2" ;
  skos:prefLabel "Weak"@en ;
.
