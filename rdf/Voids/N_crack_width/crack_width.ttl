crack_width:
  rdf:type owl:Ontology ;
  dc:source "natsoil CSIRO database - crack width table"^^<http://www.w3.org/2001/XMLSchema#string> ;
  dct:created "2015-11-30"^^<http://www.w3.org/2001/XMLSchema#date> ;
  dct:creator <mailto:xavier.butcher@csiro.au> ;
  rdfs:comment "A crack is a general term for pore space and other openings in soils not occupied by solid mineral matter."^^<http://www.w3.org/2001/XMLSchema#string> ;
  rdfs:label "Crack Width"^^<http://www.w3.org/2001/XMLSchema#string> ;
  rdfs:seeAlso "National Committee on Soil and Terrain (2009), ‘Australian soil and land survey field handbook (3rd edn).’ (CSIRO Publishing: Melbourne)"^^<http://www.w3.org/2001/XMLSchema#string> ;
  owl:imports dc: ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.
crack_width:coarse
  rdf:type skos:Concept ;
  rdfs:label "Coarse" ;
  <http://www.w3.org/2001/XMLSchema#maxExclusive> "20"^^<http://www.w3.org/2001/XMLSchema#string> ;
  <http://www.w3.org/2001/XMLSchema#minInclusive> "10"^^<http://www.w3.org/2001/XMLSchema#string> ;
  skos:definition "Coarse, 10-20mm" ;
  skos:notation "3" ;
  skos:prefLabel "Coarse"@en ;
.
crack_width:extremely-coarse
  rdf:type skos:Concept ;
  rdfs:label "Extremely Coarse" ;
  <http://www.w3.org/2001/XMLSchema#maxExclusive> "100"^^<http://www.w3.org/2001/XMLSchema#string> ;
  <http://www.w3.org/2001/XMLSchema#minInclusive> "50"^^<http://www.w3.org/2001/XMLSchema#string> ;
  skos:definition "Extremely coarse, >50mm" ;
  skos:notation "5" ;
  skos:prefLabel "Extremely Coarse"@en ;
.
crack_width:fine
  rdf:type skos:Concept ;
  rdfs:label "Fine" ;
  <http://www.w3.org/2001/XMLSchema#maxExclusive> "5"^^<http://www.w3.org/2001/XMLSchema#string> ;
  <http://www.w3.org/2001/XMLSchema#minInclusive> "0"^^<http://www.w3.org/2001/XMLSchema#string> ;
  skos:definition "Fine, <5mm" ;
  skos:notation "1" ;
  skos:prefLabel "Fine"@en ;
