  skos:notation "2" ;
  skos:prefLabel "Medium"@en ;
.
crack_width:very-coarse
  rdf:type skos:Concept ;
  rdfs:label "Very Coarse" ;
  <http://www.w3.org/2001/XMLSchema#maxExclusive> "50"^^<http://www.w3.org/2001/XMLSchema#string> ;
  <http://www.w3.org/2001/XMLSchema#minInclusive> "20"^^<http://www.w3.org/2001/XMLSchema#string> ;
